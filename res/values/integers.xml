<!-- integers.xml is present in mobile/android/base/resources/values and
     is not owned by Android Sync.  We include this so we can make
     the Android Sync APK handle any health report requests *before*
     any installed Fennec APK.  This is strictly to help with
     debugging. -->

<resources>

    <!-- Fennec has priority 100 (and bigger goes first). -->
    <integer name="healthreport_receiver_priority">500</integer>

</resources>
